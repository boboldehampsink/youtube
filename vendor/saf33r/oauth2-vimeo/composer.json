{
  "name": "saf33r/oauth2-vimeo",
  "description": "Vimeo Client Provider for The PHP League OAuth 2.0 Client",
  "license": "MIT",
  "authors": [
    {
      "name": "Safeer Ahmed",
      "homepage": "http://safeerahmed.uk"
    }
  ],
  "keywords": [
    "oauth",
    "oauth2",
    "client",
    "authorization",
    "authorisation",
    "vimeo"
  ],
  "require": {
    "php": ">=5.5.0",
    "league/oauth2-client": "~1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "~0.9",
    "squizlabs/php_codesniffer": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "Saf33r\\OAuth2\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Saf33r\\OAuth2\\Client\\Test\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
