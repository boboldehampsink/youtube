{
    "name": "boboldehampsink/youtube",
    "description": "YouTube Upload Plugin for Craft CMS",
    "authors": [
        {
            "name": "Bob Olde Hampsink",
            "email": "b.oldehampsink@itmundi.nl",
            "homepage": "http://github.com/boboldehampsink",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "dukt/oauth",
                "version": "1.0.76",
                "dist": {
                    "url": "https://github.com/dukt/craft-oauth/archive/1.0.76.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/dukt/craft-oauth",
                    "type": "git",
                    "reference": "1.0.76"
                }
            }
        }
    ],
    "type": "craft-plugin",
    "require": {
        "php": "^5.3",
        "ext-gd": "*",
        "ext-mbstring": "*",
        "ext-mcrypt": "*",
        "composer/installers": "~1.0",
        "google/apiclient": "^1.1.5",
        "dukt/oauth": "^1.0.76"
    },
    "scripts": {
        "post-install-cmd": [
            "if test -d vendor/dukt/oauth; then mv vendor/dukt/oauth ../ && rm -rf vendor/dukt; else echo 1; fi"
        ],
        "post-update-cmd": [
            "if test -d vendor/dukt/oauth; then mv vendor/dukt/oauth ../ && rm -rf vendor/dukt; else echo 1; fi"
        ]
    }
}
